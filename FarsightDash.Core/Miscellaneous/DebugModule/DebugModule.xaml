<UserControl x:Class="FarsightDash.BaseModules.Miscellaneous.DebugModule.DebugModule"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FarsightDash.BaseModules.Miscellaneous.DebugModule"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox Grid.Row="0" Name="DataTextBox"></TextBox>
        <Button Grid.Row="1" Content="Emit Data" Click="ButtonBase_OnClick"></Button>
    </Grid>
</UserControl>
